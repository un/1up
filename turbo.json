{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "globalDependencies": [
    ".env"
  ],
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**"
      ]
    },
    "dev:package": {
      "dependsOn": [],
      "cache": false,
      "persistent": false
    },
    "dev": {
      "dependsOn": [
        "^dev:package"
      ],
      "cache": false,
      "persistent": false,
      "env": [
        "BASE_URL",
        "NEXT_PUBLIC_URL",
        "BACKEND_URL",
        "NEXT_PUBLIC_BACKEND_URL",
        "EXPO_PUBLIC_BACKEND_URL",
        "AUTH_SECRET_KEY",
        "DATABASE_HOST",
        "DATABASE_USERNAME",
        "DATABASE_PASSWORD"
      ]
    },
    "format": {
      "outputs": [
        ".cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "ios": {
      "cache": false,
      "interactive": true,
      "env": [
        "EXPO_PUBLIC_BACKEND_URL"
      ]
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "BASE_URL",
    "NEXT_PUBLIC_URL",
    "BACKEND_URL",
    "NEXT_PUBLIC_BACKEND_URL",
    "EXPO_PUBLIC_BACKEND_URL",
    "AUTH_SECRET_KEY",
    "DB_HOST",
    "DB_USERNAME",
    "DB_PASSWORD",
    "DB_DATABASE"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}