{
  "name": "@potential/trpc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./": {
      "import": "./src/",
      "require": "./src/",
      "default": "./src/"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev:package": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.1",
    "@potential/auth": "workspace:*",
    "@potential/consts": "workspace:*",
    "@potential/db": "workspace:*",
    "@potential/email": "workspace:*",
    "@potential/env": "workspace:*",
    "@potential/storage": "workspace:*",
    "@potential/utils": "workspace:*",
    "@potential/validators": "workspace:*",
    "@trpc/server": "catalog:",
    "ai": "^4.2.2",
    "better-auth": "catalog:",
    "superjson": "2.2.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@potential/eslint-config": "workspace:*",
    "@potential/prettier-config": "workspace:*",
    "@potential/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@potential/prettier-config"
}